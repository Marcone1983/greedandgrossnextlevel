{
  "cli": {
    "version": ">= 16.13.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      },
      "prebuildCommand": "bash scripts/fix-react-native-maven.sh"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      },
      "cache": {
        "disabled": true
      },
      "env": {
        "REACT_NATIVE_MAVEN_REPO_PATH": "node_modules/react-native/android"
      },
      "prebuildCommand": "bash scripts/fix-react-native-maven.sh"
    }
  },
  "submit": {
    "production": {}
  }
}
